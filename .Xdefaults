! from http://www.freedesktop.org/wiki/Standards_2fXSettingsRegistry
!      http://www.freedesktop.org/wiki/Specifications/XSettingsRegistry
!
!    Xft settings
!   ==============
!
! These settings can be shared by all Xft clients.

! Xft/Antialias
! Whether to antialias Xft fonts; 0no, 1yes, -1=default
!  * int -1
Xft.antialias:  1

! Xft/Hinting
! Whether to hint Xft fonts; 0no, 1yes, -1=default
!  * int -1
Xft.hinting: 1

! Xft/HintStyle
! What degree of hinting to use; "hintnone",
! "hintslight", "hintmedium", or "hintfull"
!  * string "hintnone"
Xft.hintstyle: hintfull

! Xft/RGBA
! Type of subpixel antialiasing; "none",
! "rgb", "bgr", "vrgb", "vbgr"
!  * string "none"
Xft.rgba: rgb

! Xft/DPI
! Resolution for Xft, in 1024 *	dots/inch. -1 to use default value
!  * int -1
Xft.dpi:       96.000000
