[user]
	email = jasan@jasan.tk
	name = Jan Sarenik
[push]
	default = current
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[pull]
	rebase = true
[rerere]
	enabled = true
[alias]
	s = status -s
	lg = log --oneline --decorate --all --graph
[color]
	ui = auto
[credential]
	helper = cache
[sendemail]
	smtpEncryption = tls
	smtpServer = smtp.gmail.com
	smtpUser = jajomojo@gmail.com
	smtpServerPort = 587
